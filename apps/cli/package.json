{
  "name": "@sst-toolkit/cli",
  "version": "1.0.0",
  "private": true,
  "description": "CLI tool for SST Toolkit",
  "type": "module",
  "bin": {
    "sst-toolkit": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@sst-toolkit/core": "workspace:*",
    "@sst-toolkit/plugin-sdk": "workspace:*",
    "@sst-toolkit/shared": "workspace:*",
    "commander": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}

